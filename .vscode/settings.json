{
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "[git-commit]": {
        "editor.rulers": [
            50,
            72
        ]
    },
    "[java]": {
        "editor.rulers": [
            80,
            100
        ]
    },
    "[python]": {
        "editor.rulers": [
            72,
            80
        ]
    },
    "[markdown]": {
        "editor.rulers": [
            80,
            120
        ]
    },
    "python.jediEnabled": false
}